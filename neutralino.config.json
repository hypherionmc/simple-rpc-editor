  "enableNativeAPI": true,
  "url": "/resources/",
  "nativeBlockList": [],
  "globalVariables": {
    "APPVERSION": "SETAPPVERSIONHERE"
  },
  "modes": {
    "window": {
      "title": "Simple RPC Config Editor - Beta",
